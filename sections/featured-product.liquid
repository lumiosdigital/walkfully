{%- liquid
  assign featured_product = section.settings.featured_product
  assign media_width = section.settings.media_width
  assign details_width = 12 | minus: media_width
  assign media_shift_class = ''
  assign image_bounds = section.settings.image_bounds
  assign media_position = section.settings.media_position
  assign media_layout_mobile = section.settings.media_layout_mobile
  assign media_layout_desktop = section.settings.media_layout_desktop
  assign disable_radius = false

  if image_bounds == 'extended'
    assign media_shift_class = ' grid__item--shift-both grid__item--shift-left-desktop'
    assign disable_radius = true
  endif

  if image_bounds == 'extended' and media_position == 'right'
    assign media_shift_class = ' grid__item--shift-both grid__item--shift-left-desktop' | replace: 'left', 'right'
  endif

  if image_bounds == 'contained' and media_layout_mobile == 'swiper' and product.media.size != 1
    assign media_shift_class = ' grid__item--shift-both'
  endif

  assign has_variant_picker = false
  assign variant_picker_block = section.blocks | where: 'type', 'variant_picker'

  if variant_picker_block != blank
    assign has_variant_picker = true
  endif

  assign has_quantity_selector = false
  assign quantity_selector_block = section.blocks | where: 'type', 'quantity_selector'

  if quantity_selector_block != blank
    assign has_quantity_selector = true
  endif
-%}
<section-product
  class="
    product
    color-{{ section.settings.color_scheme }}
    grid
    {% render 'section-classes', settings: section.settings -%}
  "
  data-section="{{ section.id }}"
  data-id="{{ section.id }}"
  data-product-id="{{ featured_product.id }}"
  data-product-url="{{ featured_product.url }}"
  data-product-handle="{{ featured_product.handle }}"
  data-media-layout-desktop="{{ media_layout_desktop }}"
  data-media-layout-mobile="{{ media_layout_mobile }}"
  data-media-bounds="{% if featured_product.gift_card? %}contained{% else %}{{ image_bounds }}{% endif %}"
  data-media-position="{{ media_position }}"
  data-should-update-url="true"
  {% if section.settings.enable_animations %}
    data-animate-elements-on-scroll
    data-animate-delay="125"
  {% endif %}
>
  {%- assign first_3d_model = featured_product.media | where: 'media_type', 'model' | first -%}
  {%- if first_3d_model -%}
    <link
      id="ModelViewerStyle"
      rel="stylesheet"
      href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css"
      media="print"
      onload="this.media='all'"
    >
    <link
      id="ModelViewerOverride"
      rel="stylesheet"
      href="{{ 'component-model-viewer-ui.css' | asset_url }}"
      media="print"
      onload="this.media='all'"
    >
  {%- endif -%}

  <div class="grid__content">
    {%- capture media_content -%}
      <div class="product__media-gallery grid__item--span-12 grid__item--span-{{ media_width }}-desktop{% unless featured_product.gift_card? %}{{ media_shift_class }}{% endunless %}">
        {% if featured_product.gift_card? and featured_product.featured_media == blank %}
          <div class="product__media-gallery-giftcard-wrapper">
            <img
              class="product__media-gallery-giftcard-image"
              src="{{ 'gift-card/card.svg' | shopify_asset_url }}"
              alt=""
              height="{{ 1200 }}"
              width="1200"
              loading="eager"
            >
          </div>
        {% else %}
          {% render 'product-media-gallery',
            prod: featured_product,
            media_aspect_ratio: section.settings.media_aspect_ratio,
            media_thumb_size: section.settings.media_thumb_size,
            media_thumb_aspect_ratio: section.settings.media_thumb_aspect_ratio,
            media_width: media_width,
            image_bounds: image_bounds,
            enable_lightbox: true,
            enable_video_looping: section.settings.enable_video_looping,
            no_radius: disable_radius,
            media_layout_desktop: media_layout_desktop,
            media_layout_mobile: media_layout_mobile,
            auto_play_videos: section.settings.auto_play_videos,
            is_featured: true
          %}
        {% endif %}
      </div>
    {%- endcapture -%}

    {%- if media_position == 'left' -%}
      {{ media_content }}
    {%- endif -%}

    <div class="product__details grid__item--span-12 grid__item--span-{{ details_width }}-desktop{% if section.settings.enable_animations %} animate animate--slide-in{% endif %}">
      <div class="product__details-content" data-product-url="{{ featured_product.url }}">
        {%- assign product_form_id = 'product-form-' | append: section.id -%}

        {%- for block in section.blocks -%}
          {%- case block.type -%}
            {%- when '@app' -%}
              <div class="product__block">
                {% render block %}
              </div>

            {%- when 'vendor' -%}
              {%- render 'product-block-vendor',
                block: block,
                prod: featured_product,
                index: forloop.index0,
                is_featured: true
              -%}

            {%- when 'rating' -%}
              {%- render 'product-block-rating',
                block: block,
                prod: featured_product,
                index: forloop.index0,
                is_featured: true
              -%}

            {%- when 'title' -%}
              {%- liquid
                assign show_as_link = false

                if featured_product != blank
                  assign show_as_link = true
                endif
              -%}
              {%- render 'product-block-title',
                block: block,
                prod: featured_product,
                index: forloop.index0,
                is_featured: true,
                show_as_link: show_as_link
              -%}

            {%- when 'price' -%}
              {%- render 'product-block-price',
                block: block,
                prod: featured_product,
                index: forloop.index0,
                is_featured: true
              -%}

            {%- when 'sku' -%}
              {%- render 'product-block-sku',
                block: block,
                prod: featured_product,
                index: forloop.index0,
                is_featured: true
              -%}

            {%- when 'text' -%}
              {%- render 'product-block-text', block: block, prod: featured_product, index: forloop.index0 -%}

            {%- when 'icon_with_text' -%}
              {%- render 'product-block-icon-with-text', block: block, prod: featured_product, index: forloop.index0 -%}

            {%- when 'variant_picker' -%}
              {% unless featured_product == blank %}
                {% assign id = 'product-variant-options-' | append: section.id %}
                {% render 'product-block-variant-picker',
                  prod: featured_product,
                  block: block,
                  id: id,
                  product_form_id: product_form_id
                %}
              {% endunless %}

            {%- when 'quantity_selector' -%}
              {% unless featured_product == blank %}
                {% assign id = 'product-quantity-selector-' | append: section.id %}
                {%- render 'product-block-quantity-selector',
                  prod: featured_product,
                  block: block,
                  product_form_id: product_form_id,
                  id: id
                -%}
              {% endunless %}

            {%- when 'buy_buttons' -%}
              {% unless featured_product == blank %}
                {%- render 'product-block-buy-buttons',
                  block: block,
                  prod: featured_product,
                  product_form_id: product_form_id,
                  media_aspect_ratio: section.settings.media_aspect_ratio,
                  has_variant_picker: has_variant_picker,
                  has_quantity_selector: has_quantity_selector,
                  show_quick_buy_widget: false,
                  show_pickup_availability: true,
                  is_featured: true,
                  id: id
                -%}
              {% endunless %}

            {%- when 'description' -%}
              {%- render 'product-block-description',
                block: block,
                prod: featured_product,
                index: forloop.index0,
                is_featured: true
              -%}

            {%- when 'accordion' -%}
              {%- render 'product-block-accordion', block: block, index: forloop.index0 -%}

            {%- when 'complementary_products' -%}
              {%- render 'product-block-complementary-products', block: block, prod: product, index: forloop.index0 -%}

            {%- when 'custom_liquid' -%}
              {% if block.settings.custom_liquid != blank %}
                <div
                  class="
                    product__block
                    {%- render 'product-block-classes', block: block -%}
                  "
                >
                  <div class="product__custom-liquid" {{ block.shopify_attributes }}>
                    {{ block.settings.custom_liquid }}
                  </div>
                </div>
              {% endif %}

            {%- when 'payment_icons' -%}
              {% if shop.enabled_payment_types.size > 0 %}
                <div
                  class="
                    product__block
                    {%- render 'product-block-classes', block: block -%}
                  "
                >
                  <div class="product__payment">
                    {% if block.settings.heading != blank %}
                      <h3 class="product__payment-title {% render 'text-control-classes', settings: block.settings %}">
                        {{ block.settings.heading }}
                      </h3>
                    {% endif %}
                    <ul class="product__payment-items reset">
                      {%- for type in shop.enabled_payment_types -%}
                        <li class="product__payment-item">
                          {{ type | payment_type_svg_tag: class: 'product__payment-item-icon' }}
                        </li>
                      {%- endfor -%}
                    </ul>
                  </div>
                </div>
              {% endif %}

            {%- when 'share' -%}
              {% unless featured_product == blank %}
                <div
                  class="
                    product__block
                    {%- render 'product-block-classes', block: block -%}
                  "
                >
                  <div class="product__share" {{ block.shopify_attributes }}>
                    {%- liquid
                      assign share_url = featured_product.selected_variant.url | default: featured_product.url | prepend: request.origin
                      assign share_type = 'products.product.share_type' | t
                    -%}
                    {% render 'share-button',
                      share_url: share_url,
                      type: share_type,
                      share_image: featured_product.image,
                      share_title: featured_product.title
                    %}
                  </div>
                </div>
              {% endunless %}
          {%- endcase -%}
        {%- endfor -%}
      </div>
    </div>

    {%- if media_position == 'right' -%}
      {{ media_content }}
    {%- endif -%}
  </div>

  {%- if first_3d_model -%}
    <script type="application/json" id="ProductJSON-{{ product.id }}">
      {{ product.media | where: 'media_type', 'model' | json }}
    </script>
  {%- endif -%}

  <script type="application/ld+json">
    {{ product | structured_data }}
  </script>
</section-product>

